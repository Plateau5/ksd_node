                var pawnValue = $('#pawnValue').val().number();
                // 清楚默认的0值
                if (oldValue == 0) {
                    _this.val('');
                }
                /*var isWrap = $('input[name="isWrap"]:checked').val();
                var minFirstScale = isWrap == 1 ? 20 : 10;
                if ((oldValue * 100 / pawnValue) < minFirstScale) {
                    _this.siblings('.tips_info').show().find('.tips_text').text('首付比例不能低于'+ minFirstScale +'%。');
