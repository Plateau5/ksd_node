        // 是否打包切换更改计算金额结果
        function isBale () {
            var baleInput = $('input[name="isWrap"]');  // 打包元素
            var firstPay = $('#firstPayAmount');        // 首付金额
            var firstScale = $('#firstPayScale');       // 首付比例
            baleInput.on('change', function () {
                var t = $(this);
                var v = Number(t.val());
                var firstScaleV = firstScale.val().trim().number();
                if (v === 1) {
                    if (firstScaleV < 20) {
//                        firstPay.siblings('.tips_info').show().find('.tips_text').text('首付比例不能低于20%。');
                        firstPay.attr('verify', 0);
                    } else {
                        firstPay.css({
                            'border-color' : '#e4e4e4'
                        }).siblings('.tips_info').hide().find('.tips_text').text('');
                        firstPay.attr('verify', 1);
                    }
                    calcfinanceAmount(true);
                } else if (v === 2) {
                    if (firstScaleV < 10) {
//                        firstPay.siblings('.tips_info').show().find('.tips_text').text('首付比例不能低于10%。');
                        firstPay.attr('verify', 0);
                    } else {
                        firstPay.css({
                            'border-color' : '#e4e4e4'
                        }).siblings('.tips_info').hide().find('.tips_text').text('');
                        firstPay.attr('verify', 1);
                    }
                    calcfinanceAmount(false);
                }
            });

        }

        // 车辆价格等金额发生变化监听
        function accountChange () {
            var elem = $('#otherFee, #pawnValue, #buyTax, #assure, #cautionMoney, #serviceCharge , #attachFinance');
            var oldValue = '';
            var firstPayAmountE = $('#firstPayAmount');     // 首付金额
