                    return false;
                } else {
                    // firstPayScale = firstPayScale;
                    _this.siblings('.tips_info').hide().find('.tips_text').text('');
                    _this.attr('verify', 1);
                }*/

            });
            firstPayAmountE.on('input', function () {
                var _this = $(this);
