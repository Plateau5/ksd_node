                            _this.siblings('.tips_info').show().find('.tips_text').text('首付金额不能大于车辆实际价格。');
                            _this.attr('verify', 0);
                            return false;
                        }
                        var firstPayScale = (newValue * 100 / pawnValue).toFixed(2);
                        firstPayScaleE.val(firstPayScale);
                        calcfinanceAmount();
