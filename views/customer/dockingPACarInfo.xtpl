<script>
    (function ($) {
        // 还款计划层的显示隐藏
        function showRepaymentPlan () {
            var btn = $('.confirm_btn, .repayment_plan_btn');
            btn.off('click').on('click', function () {
                var _this = $(this);
                var p = $('.repayment_plan_table');
                if (p.is(':hidden')) {
                    p.show();
                } else {
                    p.hide();
                }
            });
        }





        $(function() {
            validateBlurEmpty();    // 输入框非空校验
            showRepaymentPlan();
        });
    })(jQuery,undefined);
</script>