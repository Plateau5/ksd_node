<!DOCTYPE html>
<html lang="en">
<head>
    {{include ("./../inc/cssSources")}}
    <link rel="stylesheet" href="{{markUri}}/static/css/orderDetails.css">
    <title>{{title}}</title>
</head>
<body>
<div id="wrapper" class="wrapper">
    <!-------- Part of header Begin -------->
    {{include ('./../inc/header')}}
    <!-------- Part of header End -------->

    <!-------- Part of main Begin -------->
    <div id="section" class="section normal_width">
        <!---- Part of slide nav Begin ---->
        {{include ('./../inc/sideNav')}}
        <!---- Part of slide nav End ---->
        <form action="/finance/file/download" id="finance_download" method="post">
            <input id="finance_download_finance_id" value="{{finance_id}}" name="finance_id" type="hidden">
            <input id="user_name" value="{{vo.user_name}}" name="user_name" type="hidden">
            <input id="material_type" value="" name="material_type" type="hidden">
        </form>
        <!---- Part of Main info Begin ---->
        <div id="main" class="main">
            <div class="crumbs_nav">
                <a href="{markUri}}/customer/loan/system" class="crumbs_item">贷款管理</a>
                <a href="{{url}}" class="crumbs_item">{{navigation}}</a>
                <a href="javascript:;" class="crumbs_item">{{vo.user_name }}</a>
                <a href="javascript:;" class="crumbs_item">查看资料</a>
            </div>
            <!-- 详情tab部分 开始 -->
            <div class="details_content">
                <div class="tab">
                    <ul class="tab_list inline_block">
                        <li class="tab_item type_normal active" data-type="0" style="border-left:0;"><span></span>进件资料</li>
                        <li class="tab_item type_others" data-type="1"><span></span>合同资料</li>
                        <li class="tab_item type_others" data-type="2"><span></span>请款资料</li>
                        <li class="tab_item type_others" data-type="3"><span></span>归档资料</li>
                    </ul>
                </div>
                <div class="details_item base_info">
                    <!-- 基本信息 Begin -->
                    {{include ('./../inc/customerBasicInfo')}}
                    <!-- 基本信息 End -->
                    <!-- 合同资料 Begin -->
                    {{include ('./../inc/compactInfo')}}
                    <!-- 合同资料 End -->
                    <!-- 请款资料部分 Begin -->
                    <div class="detail_box" style="display: none">
                        <div class="classify_item">
                            <div class="classify_content">
                                <div class="classify_title">车辆信息</div>
                                <ul class="items_list">
                                    <li class="item_row">
                                        <div class="item_name">风险类型：</div>
                                        <div class="item_value">2</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">车架号：</div>
                                        <div class="item_value">自行安装</div>
                                    </li>
                                </ul>
                                <div class="classify_title">车款审批</div>
                                <ul class="items_list">
                                    <li class="item_row">
                                        <div class="item_name">总额：</div>
                                        <div class="item_value">2018</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">账户用途：</div>
                                        <div class="item_value">白海安</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">车架贷款额：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">账户类型：</div>
                                        <div class="item_value">宝马5系</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">账户名：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">银行卡号：</div>
                                        <div class="item_value">宝马5系</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">开户行：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">备注：</div>
                                        <div class="item_value">宝马5系</div>
                                    </li>
                                </ul>
                                <div class="classify_title">北京返点支出</div>
                                <ul class="items_list">
                                    <li class="item_row">
                                        <div class="item_name">总额：</div>
                                        <div class="item_value">2018</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">账户用途：</div>
                                        <div class="item_value">白海安</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">GPS返点：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">账户类型：</div>
                                        <div class="item_value">宝马5系</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">账户名：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">银行卡号：</div>
                                        <div class="item_value">宝马5系</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">开户行：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">商户名称：</div>
                                        <div class="item_value">新车、二手车</div>
                                    </li>
                                    <li class="item_row">
                                        <div class="item_name">备注：</div>
                                        <div class="item_value">宝马5系</div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <!-- 详情页按钮逻辑 Begin -->
                    {{include ('./../inc/customerDetailBtn')}}
                    <!-- 详情页按钮逻辑 End -->

                </div>
            </div>
            <!-- 详情图片附件 Begin -->
            <div class="details_content">
                <div class="tab">
                    <ul class="tab_list inline_block">
                        <li class="tab_item type_normal active" data-type="0" style="border-left:0;"><span></span>进件附件</li>
                        <li class="tab_item type_others" data-type="1"><span></span>合同附件</li>
                        <li class="tab_item type_others" data-type="2"><span></span>请款附件</li>
                        <li class="tab_item type_others" data-type="3"><span></span>归档附件</li>
                    </ul>
                </div>
                <!-- 附件部分 Begin -->
                <div class="details_item">
                    <div class="detail_box">
                        {{#if (file_list.length > 0) }}
                        <ul class="img_list" id="img_list">
                            {{#each (file_list) }}
                            <a href="javascript:;" target="_blank">
                                <div class="list_item imgDetail" style="margin-top:0;" title="">
                                    <div class="detailBox img_item">
                                        <img class=" detailImgBox" data-original="{{this.thumbnail}}" src="{{this.thumbnail_120_90}}" alt="{{this.file_name}}"/>
                                    </div>
                                    <div class="img_desc nor_wrap">{{this.file_name}}</div>
                                </div>
                            </a>
                            <!--<div class="detailBox inline">-->
                                    <!--<div class="list_item detailImgBox cursor">-->
                                        <!--<a href="javascript:;" data-type="imgBox"><img data-original="{{this.thumbnail}}" src="{{this.thumbnail_120_90}}" alt="{{this.file_name}}"></a>-->
                                        <!--<div class="img_desc nor_wrap">{{this.file_name}}</div>-->
                                    <!--</div>-->

                            <!--</div>-->

                            {{/each}}
                            <!--<li class="list_item">-->
                                <!--<a href="javascript:;"><img src="../static/img/a.jpg" alt=""></a>-->
                                <!--<div class="img_desc nor_wrap">身份证</div>-->
                            <!--</li>-->
                        </ul>
                        {{/if}}
                    </div>
                    <div class="detail_footer">
                        <a href="javascript:" class="customer_detail_download detail_high_btn download_file" alt="4" lang="{{finance_id}}">全部下载</a>
                        <!--<a href="javascript:" class="detail_high_btn">全部下载</a>-->
                    </div>
                </div>
            </div>
            <!-- 详情图片附件 End -->
            <!-- 操作记录 Begin -->
            {{include ('./../inc/operateLogs')}}
            <!-- 操作记录 End -->

        </div>
        <!---- Part of Main info End ---->
    </div>
    <!-------- Part of main End -------->

    <!-------- Part of footer Begin -------->
    <!--<div id="footer" class="footer"></div>-->
    <!-------- Part of footer End -------->
</div>
<div class="mask" style="display: none;"></div>
</body>
{{include ('./../inc/jsSources')}}
<script src="{{markUri}}/static/js/finance/img_detail.js"></script>
<script>
    (function () {
        var currentTime = new Date().format('yyyy/MM/dd hh:mm');
        var currentPerson = document.getElementById("currentTime");
        if (currentPerson != null || currentPerson != undefined) {
            currentPerson.innerHTML = currentTime;
        }
    })(window, undefined);
        $(function () {
            viewLargeImage('#img_list');       // 查看大图
            viewLargeImage('.log_item');
        });
</script>
</html>
