<script>
    // 获取城市数据
    var cityData = getAddressData();
    (function ($) {

        // 根据是否有担保人更改保存并继续的跳转路径
        function isHaveGuarantor () {
            var guarantor = $('.isSponsor');
            var confirm = $('#saveAndGoNext');
            var nextPath = '';
            guarantor.on('change', function () {
                var _this = $(this);
                var v = $.trim(_this.val());
                if (v == 1) {
                    nextPath = markUri + '/docking/pingan/guarantor';
                } else if (v == 2) {
                    nextPath = markUri + '/docking/pingan/files';
                }
                confirm.data('next', nextPath);
            });
        }

        // 根据是否有车贷展示现有车系
        function isHasCarLoan () {
            var firstBuy = $('input[name="isFirstBuy"]');      // 是否为首次购车单选按钮
            var firstBuyC = $('.is_first_buy_item');     // 是否首次购车父级元素
            var hascarloan = $('input[name="hascarloan"]');     // 是否有车贷单选按钮
            var hascarloanC = $('.hascarloan_item');        // 是否有车贷父级元素
            var oencarbrand = $('#oencarbrand');        // 现有车系输入框
            var oencarbrandC = $('.oencarbrand_item');       // 现有车系父级框
            hascarloan.on('click', function () {
                var _this = $(this);
                var val = _this.val().trim();
                if (val == 1) {
                    oencarbrand.removeClass('required').addClass('required');
                    oencarbrandC.show();
                } else {
                    oencarbrand.removeClass('required');
                    oencarbrandC.hide();
                }
            });

            firstBuy.on('click', function () {
                var _this = $(this);
                var val = _this.val().trim();
                if (val == 1) {
                    hascarloanC.hide();
                    hascarloan.attr('disabled', 'disabled');
                    oencarbrand.removeClass('required');
                    oencarbrandC.hide();
                } else {
                    hascarloanC.show();
                    hascarloan.removeAttr('disabled');
                    oencarbrand.removeClass('required').addClass('required');
                    oencarbrandC.show();
                }
            });

        }

        $(function() {
            intOrFloat($('#familyInCome, #familyExpend'), 9999999.99);
            initDateAll('.birthday');      // 加载日期插件
            isHaveWork();       // 工作模块的显示隐藏
            bindSubmitEvent();      // 提交
            isMarriage();       // 显示隐藏配偶信息
            verifyGender();     // 设置性别
            isHaveGuarantor();      // 担保人监听
            getOccupation(cityData.data_occupation);      // 展示职业
            getProvince(cityData.data_province);        // 展示省份数据
            createCityList();       // 根据选中省份获取对应城市
            verifyPhone('.phone');      // 手机号码校验
            validateBlurEmpty();    // 输入框非空校验
            verifyLicense('.ID_num');       // 证件号码校验
            isHasCarLoan();     // 是否有车贷更改
        });
    })(jQuery,undefined);
</script>