        /**
         * Normal时间插件
         * @author Arley Joe 2017-8-9 14:50:11
         * @param target {string} 目标触发元素
         */
        function initDateAll (target) {
            $(target).jeDate({
                isinitVal : false,
                festival : false,
                ishmsVal : false,
                format : "YYYY-MM-DD",
                zIndex : 3000,
                isClear : true
            });
            $(target).addClass('datainp wicon');
        }

        //初始加载时间默认部门展开
        function initSelected () {
            var t = $(".list_box").find(".selected");
            var tp = t.parent(".sub_list");
            var levelTop = t.parent(".sub_list").parent(".list_item");
            if (tp.length != 0) {
                tp.show();
                levelTop.addClass("selected");
            }
        }


        //获取创建部门选择数据
        var level = 0;
        function showDepartmentData (data) {
            level++;
            var ele = "";
            ele += '<div class="sub_list">';
            var hisId = $.trim($(".department_desc").attr("value"));
            for (var i = 0, len = data.length; i < len; i++) {
                if (data[i].children.length == 0 || data[i].children == [] || data[i].children == undefined) {
                    ele += '<div class="list_item '+ ((hisId == data[i].id) && "selected") +'" data-parent="'+ data[i].parent_id +'" data-value="'+ data[i].id +'"><span>'+ data[i].name +'</span></div>';
                } else {
                    var subEle = showDepartmentData(data[i].children);
                    ele += '<div class="list_item '+ ((hisId == data[i].id) && "selected") +' has_children" data-parent="'+ data[i].parent_id +'" data-value="'+ data[i].id +'"><em class="arrow_icon"></em><span>'+ data[i].name +'</span>'+ subEle +'</div>'
                }
            }
            ele += '</div>';

            return ele;
        }

        //展开或者隐藏部门列表
        function dropDownDepList() {
            var target = $(".dep_list");
            var body = $("body:not('.list_box')");
            target.off("click").on("click", function (e) {
                var e = e || window.event;
                e.stopPropagation();
                e.preventDefault();
                var t = target.find(".list_box");
                if (t.is(':hidden')) {
                    t.show();
                } else {
                    t.hide();
                }
            });
            body.off("click").on("click", function () {
                var t = $(".list_box");
                if (!t.is(':hidden')) {
                    t.hide();
                }
            });
        }


        var chooseId = '';//选择的部门ID
        //选中当前部门
        function chooseDep () {
            var t = $(".list_box").find(".list_item");
            t.each(function () {
                var _this = $(this);
                _this.off("click").on("click", function (e) {
                    var e = e || window.event;
                    e.stopPropagation();
                    e.preventDefault();
                    var target = $(this);
                    var depName = [];
                    var depId = target.attr("data-value");
                    chooseId = depId;
                    var thisName = target.find(">span").text();
                    target.parents(".list_item").each(function () {
                        var tp = $(this);
                        var tpName = tp.find(">span").text();
                        tpName && depName.push(tpName);
                    });
                    depName.push(thisName);
                    target.parents(".list_box").find(".list_item").removeClass("selected");
                    target.addClass("selected");
                    $("#department_id").val(depId);
                    $(".department_desc").text(depName.join("-")).attr("value", depId);
                    t.parents(".list_box").hide();
                });
            });
        }
        //展开下级部门
        function slideDownChildrenDep () {
            var target = $(".has_children >.arrow_icon");
            target.each(function () {
                var $target = $(this);
                $target.off("click").on("click", function (e) {
                    var _this = $(this);
                    var subList = _this.siblings(".sub_list");
                    var e = e || window.event;
                    e.stopPropagation();
                    e.preventDefault();
                    if (subList.is(":hidden")) {
                        _this.parents(".has_children").siblings().find(">.sub_list").slideUp(300).end().find(">.arrow_icon").removeClass("up_arrow");
                        subList.slideDown(300);
                        _this.addClass("up_arrow");
                    } else {
                        //depId = target.attr("data-value");
                        _this.parents(".has_children").siblings().find(">.sub_list").slideUp(300);
                        subList.slideUp(300);
                        _this.removeClass("up_arrow");
                    }
                });
            });



        }

        $(function () {
            initDateAll('#demo1, #demo2, #demo3');
            dropDownDepList();
            var listEles = showDepartmentData(depList[0].children);
            $(".list_box").html(listEles);
            initSelected();
            //setLevelOneHover();
           slideDownChildrenDep();
            chooseDep();
        });
    })(jQuery, undefined);
