<script>
    const TYPE = true;
    var PARENTID = '${vo.parent_id}',
        PARENTNAME = '${vo.parent_name}';
    var width = $('#product_info_create').width();
    var left = $('.col-xs-2').width();
    $('.add_btn').css('marginLeft',-left - 140 + 'px');
    $('.remarks_box textarea').css('margin-left',left + 35 + 'px');
    $('.form_left').css('margin-left',left - 5 + 'px');
    $('#add_btn_n').click(function(){
        send_product_form(contextPath + "/api/product/createAndPublish",1);
    });
    $('#add_btn_y').click(function(){
        send_product_form(contextPath + "/api/product/create",0);
    });
    $('.rate_box').css('display','block');
    var rate_length = $('.rate_box').length;
    if(rate_length != '0'){
        $('.rate_add').css('marginLeft',left - 5  + 'px');
    }
    $('.rate_box').css('marginLeft',left - 5 + 'px');
    $('.rate_box').eq(0).css('marginLeft' , '');
    $('.rate_add input').val("");
    var downpayment_length = $('.downpayment_box').length;
    if(downpayment_length != '0'){
        $('.downpayment_add').css('marginLeft',left - 5  + 'px');
    }
    $('.downpayment_box').css('marginLeft',left - 5 + 'px');
    $('.downpayment_box').eq(0).css('marginLeft' , '');
    $('.downpayment_add input').val("");
    pageJump(".go_forward", {
        parent_id : PARENTID
    });
</script>