<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>产品管理</title>
    {{include ("./../inc/metaData")}}
    <link rel="stylesheet" href="{{markUri}}/static/css/employee/invite.css"/>
    <link rel="stylesheet" href="{{markUri}}/static/css/employee/information.css"/>
    <link rel="stylesheet" href="{{markUri}}/static/css/product/create.css">
    <script src="{{markUri}}/static/js/common/validate_form.js"></script>
    <script src="{{markUri}}/static/js/product/create.js"></script>
    <style type="text/css">
        body{overflow-x: hidden;}
        .formML{margin-bottom:50px;margin-top:35px;padding-left:4%;}
        .col-xs-10,.col-xs-8,.col-xs-7,div[class*="col-xs-2"],div[class*="col-xs-3"]{margin-left:-40px;}
        .col-xs-8 textarea{height:86px;resize: none;-webkit-scroll-y:auto;}
        .mask{width:100%;position: absolute;z-index:1;display:none;left:0;top:0;}
    </style>
</head>
<body>
<div class="mask"></div>
{{include ('./../inc/header')}}

<!--container start-->

<div class="container minWidth">
    <div class="row section">
        <!--navLeft start-->
        {{include ('./../inc/sideNav')}}

        <!--navLeft end-->

        <!--listCon start-->
        <div class="" style="margin-left:0;padding-left:180px;">
            <div class="listConHeader inviteCon">
                <ul>
                    <li class="inline colorB"><a class="TS" href="{{markUri}}/supplier/organization/system" style="cursor: pointer;">金融机构</a></li>
                    <li class="inline colorB before"><a class="TS go_forward" data-url="{{markUri}}/supplier/organization/unpublishedProducts" style="cursor: pointer;">产品管理</a></li>
                    <li class="inline before"><a href="javascript:;">发布新产品</a></li>
                </ul>
            </div>
            <!--form start-->
            <div class="inviteForm inviteConB formMT">

                <form class="form-horizontal formML" id="product_info_create">
                    <input type="hidden" name="parent_id" value="{{vo.parent_id}}">
                    <input type="hidden" name="parent_name" value="{{vo.parent_name}}">
                    <div class="form-group">
                        <label class="col-xs-2 control-label inviteLabel">
                            <span class="need">*</span>产品名称：</label>
                        <div class="col-xs-5">
                            <input type="text" class="form-control" id="name" name="name" value="{{vo.name}}" placeholder="请输入产品名称" maxlength="20" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='请输入产品名称';this.style.color='#A4A8AB';}"/>
                        </div>
                        <span class="formProm"></span>
                        <div class="trueImg"></div>
                        <span class="formError"></span>
                    </div>
                    <div class="form-group">
                        <label class="col-xs-2 control-label inviteLabel">产品介绍：</label>
                        <div class="col-xs-5">
                            <input type="text" class="form-control" id="introduce" name="introduce" placeholder="请输入产品介绍(12字以内)" maxlength="12" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='请输入产品介绍(12字以内)';this.style.color='#A4A8AB';}"/>
                        </div>
                        <span class="formProm"></span>
                        <div class="trueImg"></div>
                        <span class="formError"></span>
                    </div>
                    <div class="form-group" id="applyto_business" style="margin-bottom:0;">
                        <label class="col-xs-2 control-label inviteLabel">
                            <span class="need">*</span>适用业务：</label>
                        <div class="col-xs-3">
                            <div class="form-control" style="border:0;background:#F5F5F5;padding:0;margin-bottom:10px;">
                                <div class="radio">
                                    {{#if (vo.applyto_business === 0) }}
                                    <input type="radio" name="applyto_business" class="radio_applyto_business" value="0" checked >
                                    {{else}}
                                    <input type="radio" name="applyto_business" class="radio_applyto_business" value="0" >
                                    {{/if}}
                                    <label>新车</label>
                                </div>
                                <div class="radio">
                                    {{#if (vo.applyto_business === 1) }}
                                    <input type="radio" name="applyto_business" class="radio_applyto_business" value="1" checked >
                                    {{else}}
                                    <input type="radio" name="applyto_business" class="radio_applyto_business" value="1" >
                                    {{/if}}
                                    <label>二手车</label>
                                </div>
                            </div>
                        </div>
                        <span class="formProm"></span>
                        <div class="trueImg"></div>
                        <span class="formError"></span>
                    </div>
                    <!--<div class="form-group">
                    <label class="col-xs-2 control-label inviteLabel">
                        <span class="need">*</span>所属机构：</label>
                    <div class="col-xs-5">
                        <select name="parent_id" id="parent_id">
                            <option value="-1" <c:if test="${vo.parent_id eq -1 }">selected</c:if> >全部</option>
                            <c:forEach items="${apply_business }" var="bean">
                                <option value="${bean.id}" <c:if test="${vo.parent_id eq bean.id }">selected</c:if> >${bean.name}</option>
                            </c:forEach>
                        </select>
                    </div>
                    <span class="formProm"></span>
                    <div class="trueImg"></div>
                    <span class="formError"></span>
                </div>-->
                    <!--<c:if test="${fn:length(city_business) > 1 }">-->
                    <div class="form-group" id="applyto_city">
                        <label class="col-xs-2 control-label inviteLabel">
                            <span class="need">*</span>适用城市：</label>
                        <div class="col-xs-8">
                            <div class="form-control" style="border:0;background:#F5F5F5;padding:0;margin-bottom:10px;">
                                {{#each (city_business) }}
                                    <div class="radio">
                                        {{#if (vo.applyto_city === this.id) }}
                                        <input type="radio" name="applyto_city" value="{{this.id}}" checked="checked" >
                                        {{else}}
                                        <input type="radio" name="applyto_city" value="{{this.id}}" >
                                        {{/if}}
                                        <label>{{this.name}}</label>
                                    </div>
                                {{/each}}
                            </div>
                        </div>
                        <span class="formProm"></span>
                        <div class="trueImg"></div>
                        <span class="formError"></span>
                    </div>
                    <!--</c:if>-->
                    <div class="form-group">
                        <label class="col-xs-2 control-label inviteLabel">
                            <span class="need">*</span>首付类型：</label>
                        <div class="col-xs-5">
                            <select name="downpayment_type" id="downpayment_select">
                                <option value="-1">全部</option>
                                {{#if (vo.downpayment_type === 1) }}
                                <option value="1" selected="selected">首付比例</option>
                                {{else}}
                                <option value="1">首付比例</option>
                                {{/if}}
                                {{#if (vo.downpayment_type === 2) }}
                                <option value="2" selected="selected">首付金额</option>
                                {{else}}
                                <option value="2">首付金额</option>
                                {{/if}}
                            </select>
                        </div>
                        <span class="formProm"></span>
                        <div class="trueImg"></div>
                        <span class="formError"></span>
                    </div>
                    {{#if (vo.downpayment_type === 1) }}
                    <div class="form-group downpayment_rate" style="margin-bottom:0; display:block;">
                    {{else}}
                    <div class="form-group downpayment_rate" style="margin-bottom:0; display:none;">
                    {{/if}}
                    <label class="col-xs-2 control-label inviteLabel">
                        <span class="need">*</span>首付比例：</label>
                    <div class="col-xs-8" style="margin-left:-40px;">
                        <div class="form-control checkBox">
                            <input type="hidden" id="downpayment_rate" name="downpayment_rate" value="">
                            {{#each (list_downpaymentrate) }}
                                <div class="check">
                                    <div class="check_img
	                                    {{#if (vo.downpayment_rate.indexOf('0') === 0 && this.id === '0') }}
                                        icon_check
                                        {{elseif (vo.downpayment_rate.indexOf(this.id) !== -1 && this.id !== '0') }}
                                            icon_check
                                        {{else}}
                                            icon_uncheck
                                        {{/if}}
                                    ">
                                    </div>
                                    <span><span class="downpayment_rate_date">{{this.id}}</span>%</span>
                                </div>
                            {{/each}}
                        </div>

            </div>
            <span class="formProm"></span>
            <div class="trueImg"></div>
            <span class="formError"></span>
        </div>
         {{#if (vo.downpayment_type === 2) }}
            <div class="form-group downpayment_money" style="display:block;">
         {{else}}
            <div class="form-group downpayment_money" style="display:none;">
         {{/if}}
        <input type="hidden" id="downpayment_money" name="downpayment_money" value="">
        <label class="col-xs-2 control-label inviteLabel">
            <span class="need">*</span>首付金额：</label>
        {{#each (downpayment_money) }}
            <div class="col-xs-10 downpayment_box">
                <div class="col-xs-6 downpayment_add_ready">
                    <input type="text" class="form-control" readonly="readonly" id="" name="" value="{{this}}" />
                    <span>元</span>
                    <span class="cursor downpayment_add_del">删除</span>
                </div>
            </div>
        {{/each}}
        {{#if (downpayment_money_size >= 5) }}
        <div class="col-xs-6 downpayment_add" style="margin-left:-40px;display:none;">
        {{else}}
            <div class="col-xs-6 downpayment_add" style="margin-left:-40px;display:block;">
        {{/if}}
            <input type="text" class="form-control" placeholder="请输入首付金额" id="" name="downpayment_money_add" value="" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,interest_rate_input);"/>
            <span>元</span>
            <span class="cursor downpayment_add_txt">添加</span>
        </div>
        <div class="col-xs-5">
            <span class="formProm"></span>
            <div class="trueImg"></div>
            <span class="formError"></span>
        </div>
    </div>
    <div class="form-group">
        <input type="hidden" id="interest_rate" name="interest_rate" value="">
        <label class="col-xs-2 control-label inviteLabel">
            <span class="need">*</span>费率：</label>
        {{#each (interest_rate_list) }}
            <div class="col-xs-10 rate_box">
                <div class="col-xs-6 rate_add_ready">
                    <input type="text" class="form-control interest_rate_num interest_rate_txt" readonly="readonly" id="" name="interest_rate_date" value="{{this}}" />
                    <span>%</span>
                    <span class="cursor rate_add_del">删除</span>
                </div>
            </div>
        {{/each}}
        <!--该处原始为页面加载完成后直接JS计算-->
        <!--<div class="col-xs-6 rate_add" style="margin-left:-40px;display:-->
        {{#if (interest_rate_size >= 5) }}
	    <div class="col-xs-6 rate_add" style="margin-left:128px;display:none;">
        {{else}}
        <div class="col-xs-6 rate_add" style="margin-left:128px;display:block;">
        {{/if}}
        <input type="text" class="form-control interest_rate_data interest_rate_txt" placeholder="请输入费率" id="" name="interest_rate_input" value="" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,interest_rate_input);"/>
        <span>%</span>
        <span class="cursor rate_add_txt">添加</span>
    </div>
    <div class="col-xs-5 e_tip_info" style="margin-left: 130px;">
        <span class="formProm"></span>
        <div class="trueImg"></div>
        <span class="formError"></span>
    </div>
</div>
<div class="form-group">
    <label class="col-xs-2 control-label inviteLabel">
        <span class="need">*</span>融资金额范围：</label>
    <div class="col-xs-2" style="margin-right:-15px;">
        <input type="text" class="form-control" id="financeamount_start" placeholder="请输入最小金额" value="{{vo.financeamount_start }}" name="financeamount_start" maxlength="5" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,financeamount_start);"/>
    </div>
    <div class="col-xs-1" style="margin-left:-2.5%;line-height: 34px;text-align: center;padding:0;">—</div>
    <div class="col-xs-2" style="margin-right:-15px;margin-left:-2.5%;padding-left:0">
        <input type="text" class="form-control" id="financeamount_end" placeholder="请输入最大金额" value="{{vo.financeamount_end }}" name="financeamount_end"  maxlength="5" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,financeamount_end);"/>
    </div>
    <div class="col-xs-1" style="margin-left:0;line-height:34px;">
        <span>万</span>
    </div>
    <span class="formProm"></span>
    <div class="trueImg"></div>
    <span class="formError"></span>
</div>
<div class="form-group" id="term" style="margin-bottom:0;">
    <input type="hidden" name="term" value="{{vo.term }}">
    <label class="col-xs-2 control-label inviteLabel">
        <span class="need">*</span>融资期限：</label>
    <div class="col-xs-9" style="margin-left:-40px;">
        <div class="form-control checkBox">
            {{#each (list_term) }}
                <div class="check">
                    <div class="check_img
	                {{#if (vo.term.indexOf('6') === 0 && this.id === '6') }}
                    icon_check
                    {{elseif (vo.term.indexOf(this.id) !== -1 && this.id !== '6') }}
                        icon_check
                    {{else}}
                        icon_uncheck
                    {{/if}}
                    "></div>
                <span>{{this.id}}</span>
        </div>
        {{/each}}
    </div>
</div>
<span class="formProm"></span>
<div class="trueImg"></div>
<span class="formError"></span>
</div>
<div class="form-group" id="carage_form">
    <label class="col-xs-2 control-label inviteLabel">
        <span class="need">*</span>车龄限制：</label>
    <div class="col-xs-2" style="margin-right:-15px;">
        <input type="text" class="form-control" id="carage_start" placeholder="请输入最小车龄" name="carage_start" value="{{vo.carage_start }}" maxlength="4" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,carage_start);"/>
    </div>
    <div class="col-xs-1" style="margin-left:-2.5%;line-height: 34px;padding:0;text-align: center;">—</div>
    <div class="col-xs-2" style="margin-right:-15px;margin-left:-2.5%;padding-left:0">
        <input type="text" class="form-control" id="carage_end" placeholder="请输入最大车龄" name="carage_end" value="{{vo.carage_end }}" maxlength="4" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,carage_end);"/>
    </div>
    <div class="col-xs-1" style="margin-left:0;line-height:34px;">
        <span>年</span>
    </div>
    <span class="formProm"></span>
    <div class="trueImg"></div>
    <span class="formError"></span>
</div>
<div class="form-group" style="margin-bottom:0;">
    <label class="col-xs-2 control-label inviteLabel" style="margin-bottom:20px;">
        年龄限制范围：</label>
    <div class="col-xs-2" style="margin-right:-15px;margin-bottom:20px;">
        <input type="text" class="form-control" maxlength="2" id="age_start" value="{{vo.age_start }}" placeholder="请输入最小年龄" name="age_start" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,age_start);"/>
    </div>
    <div class="col-xs-1" style="margin-left:-2.5%;line-height: 34px;margin-bottom:20px;padding:0;text-align: center;">—</div>
    <div class="col-xs-2" style="margin-right:-15px;margin-bottom:20px;margin-left:-2.5%;padding-left:0">
        <input type="text" class="form-control" maxlength="2" id="age_end" value="{{vo.age_end }}" placeholder="请输入最大年龄" name="age_end" onfocus="this.placeholder='';this.style.color='#535E6A';" onblur="if(this.placeholder=='') {this.placeholder='';this.style.color='#A4A8AB';}" onkeypress="IsNum(event,age_end);"/>
    </div>
    <div class="col-xs-3" style="margin-left:0;line-height:34px;margin-bottom:20px;">
        <span>岁</span>
        <span class="cursor add_txt">备注</span>
    </div>
    <span class="formProm" style="margin-bottom:20px;"></span>
    <div class="trueImg" style="margin-bottom:20px;"></div>
    <span class="formError" style="margin-bottom:20px;"></span>
    <div class="col-xs-8 remarks_box"><textarea name="age_remark" placeholder="请输入内容" id="age_remark" class="form-control" maxlength="400">{{vo.age_remark }}</textarea></div>
</div>
<div class="form-group" id="addition_finance" style="margin-bottom:0;">
    <input type="hidden" name="addition_finance" value="">
    <label class="col-xs-2 control-label inviteLabel">附加可融项目：</label>
    <div class="col-xs-7">
        <div class="form-control checkBox">
            <div class="check">
                <div class="check_img
                {{#if (vo.addition_finance.indexOf('GPS') !== -1) }}
                icon_check
                {{else}}
                    icon_uncheck
                {{/if}}
                "></div>
            <span>GPS</span>
        </div>
        <div class="check">
            <div class="check_img
            {{#if (vo.addition_finance.indexOf('保险') !== -1) }}
                icon_check
            {{else}}
                icon_uncheck
            {{/if}}
            "></div>
        <span>保险</span>
    </div>
    <div class="check">
        <div class="check_img
        {{#if (vo.addition_finance.indexOf('服务费') !== -1) }}
            icon_check
        {{else}}
            icon_uncheck
        {{/if}}
        "></div>
    <span>服务费</span>
</div>
<div class="check">
    <div class="check_img
    {{#if (vo.addition_finance.indexOf('人身意外险') !== -1) }}
        icon_check
    {{else}}
        icon_uncheck
    {{/if}}
    "></div>
<span>人身意外险</span>
</div>
<div class="check">
    <div class="check_img
    {{#if (vo.addition_finance.indexOf('购置税') !== -1) }}
        icon_check
    {{else}}
        icon_uncheck
    {{/if}}
    "></div>
<span>购置税</span>
</div>
</div>
</div>
<span class="formProm"></span>
<div class="trueImg"></div>
<span class="formError"></span>
</div>
<div class="form-group">
    <label class="col-xs-2 control-label inviteLabel">
        <span class="need">*</span>审批速度：</label>
    <div class="col-xs-5">
        <select id="approve_speed" name="approve_speed">
            {{#if (vo.approve_speed === 1) }}
            <option value="1" selected="selected">1-20分钟</option>
            {{else}}
            <option value="1">1-20分钟</option>
            {{/if}}
            {{#if (vo.approve_speed === 2) }}
            <option value="2" selected="selected">1-3个小时</option>
            {{else}}
            <option value="2">1-3个小时</option>
            {{/if}}
            {{#if (vo.approve_speed === 3) }}
            <option value="3" selected>半个工作日</option>
            {{else}}
            <option value="3">半个工作日</option>
            {{/if}}
            {{#if (vo.approve_speed === 4) }}
            <option value="4" selected>1个工作日</option>
            {{else}}
            <option value="4">1个工作日</option>
            {{/if}}
            {{#if (vo.approve_speed === 5) }}
            <option value="5" selected>1-2个工作日</option>
            {{else}}
            <option value="5">1-2个工作日</option>
            {{/if}}
            {{#if (vo.approve_speed === 6) }}
            <option value="6" selected>3个工作日</option>
            {{else}}
            <option value="6">3个工作日</option>
            {{/if}}
        </select>
    </div>
    <span class="formProm"></span>
    <div class="trueImg"></div>
    <span class="formError"></span>
</div>
<div class="form-group" id="material" style="margin-bottom:0;">
    <input type="hidden" name="material" value="">
    <label class="col-xs-2 control-label inviteLabel">
        <span class="need">*</span>所需材料：</label>
    <div class="col-xs-10">
        <div class="form-control checkBox">
            {{#each (list_material) }}
                <div class="check" >
                    <div class="check_img
	                {{#if (vo.material.indexOf(this.id) !== -1) }}
                        icon_check
                    {{else}}
                        icon_uncheck
                    {{/if}}
                    " id="{{this.id }}"></div>
                <span>{{this.name}}</span>
        </div>
        {{/each}}
    </div>
</div>
<div class="col-xs-10 form_left">
    <span class="formProm"></span>
    <div class="trueImg"></div>
    <span class="formError"></span>
</div>
</div>

<div class="form-group" id="outsettling">
    <input type="hidden" name="outsettling_province" value="{{vo.outsettling_province }}">
    <input type="hidden" name="outsettling_city" value="{{vo.outsettling_city }}">
    <label class="col-xs-2 control-label inviteLabel">
        可抵押城市：</label>
    <div class="col-xs-5" style="position: relative;">
        <input type="text"  class="form-control cursor city"
            value="{{#if (vo.outsettling_province_count > 0) }}{{vo.outsettling_province_name }}等{{vo.outsettling_province_count }}个省份{{/if}}"
            readonly="readonly" placeholder="请选择" style="background-color:#fff;">
        <img class="city_img" src="/static/img/product/province.png" alt="">
        <div class="select_city_box">
            <div class="box_top">
                <div class="box_top_left province_btn cur_city">省份</div>
                <div class="box_top_right city_btn">城市</div>
            </div>
            <div class="box_middle">
                <div class="box_middle_left_container display" id="outsettling_province" >
                    {{#each (list_province) }}
                        <div class="container_txt_box">
                            <div class="check_img
	                        {{#if (vo.outsettling_province.indexOf(this.id) !== -1) }}
                                icon_check
                            {{else}}
                                icon_uncheck
                            {{/if}}
                            cursor select_province_img" id="{{this.id }}"></div>
                        <div class="container_txt">{{this.name}}</div>
                </div>
                {{/each}}
            </div>
            <div class="box_middle_right_container" id="outsettling_city" >
                <div class="container_city_box">
                    {{#each (list_city) }}
                        <div class="city_box
                        {{#if (vo.outsettling_province.indexOf(this.id) !== -1) }}
                        display
                        {{/if}}
                        " id="{{this.id }}">
                        <div class="container_city_box_top">{{this.name}}</div>
                        <div class="container_city_box_middle">
                            {{#each (this.city_list) }}
