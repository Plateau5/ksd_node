                        <div class="detail_about_label" style="vertical-align: top;">评估描述：</div>
                        <div class="detail_about_data"  style="line-height: 25px;padding-top:12px;">{{vo.assess_desc}}</div>
                    </div>
                    {{/if}}
                    {{#if (vo.bei_hu !== null && vo.bei_hu !== '' && vo.bei_hu || vo.bei_hu === 0) }}
