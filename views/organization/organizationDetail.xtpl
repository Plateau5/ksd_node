                {{/if}}
                    <div class="detail_about_title">
                        <div class="detail_about_label">附件资料</div>
                        <div class="rf_30">
                            {{#if (change_type === 7 && verifyCode(1505)) }}
                            <a href="javascript:" class="detail_title_btn edit_btn edit_material" lang="{{vo.id}}">编辑</a>
