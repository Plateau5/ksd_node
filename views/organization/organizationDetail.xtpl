    <link rel="stylesheet" href="{{markUri}}/static/css/supplier.css">
</head>
<body>
<div id="wrapper" class="wrapper">
    <!-------- Part of header Begin -------->
    {{include ('./../inc/header')}}
    <!-------- Part of header End -------->

    <!-------- Part of main Begin -------->
    <div id="section" class="section normal_width">
        <!---- Part of slide nav Begin ---->
        {{include ('./../inc/sideNav')}}
        <!---- Part of slide nav End ---->

        <!---- Part of Main info Begin ---->
        <div id="main" class="main org_detail">
            <div class="crumbs_nav">
                <a href="javascript:;" class="crumbs_item">机构管理</a>
                <a href="javascript:;" class="crumbs_item">先锋太盟</a>
            </div>
            <div class="org_detail_header">
                <div class="header_left">
                    <div class="header_left_img">
                        <img src="http://loan-file.oss-cn-beijing.aliyuncs.com/qita.png" alt="">
                    </div>
                    <div class="header_txt">
                        <div class="header_txt_name">
                            <div class="header_txt_top">先锋太盟</div>
                            <div class="header_txt_bottom">
                                <div class="header_txt_newcar">新车</div>
                                <div class="header_txt_usedcar">二手车</div>
                            </div>
                        </div>

                        <div class="header_right">
                            <div class="header_personal">
                                <span>创建人：</span>
                                <span>沈国彬</span>
                            </div>
                        </div>
                        <div class="header_right">
                            <div class="header_time">
                                <span>创建时间：</span>
                                <span>2018-6-15 9:56</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="delete_org" id="delete_org" lang="">
