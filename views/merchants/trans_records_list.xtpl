<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    {{include ("./../inc/cssSources")}}
    <link rel="stylesheet" href="{{markUri}}/static/dialog/dialog-layer.css">
    <link rel="stylesheet" href="{{markUri}}/static/jedate/skin/jedate.css"/>
    <link rel="stylesheet" href="{{markUri}}/static/css/merchants.css">
    <title>查询交易列表</title>
</head>
<body>
<div id="wrapper" class="wrapper">
    <!-------- Part of header Begin -------->
    {{include ('./../inc/header')}}
    <!-------- Part of header End -------->
    <!-------- Part of main Begin -------->
    <div id="section" class="section normal_width">
        <!---- Part of slide nav Begin ---->
        {{include ('./../inc/sideNav')}}
        <!---- Part of slide na End ---->
        <!---- Part of Main info Begin ---->
        <div id="main" class="main merchants">
            <div class="crumbs_nav">
                <a href="{{markUri}}/loan/system" class="crumbs_item">放款管理</a>
                <a href="" class="crumbs_item last_child">查询交易</a>
            </div>
            <div class="filtrate merchants_filtrate">
                <div class="filtrate_item date_conditions inline_block">
                    <div class="conditions_name inline_block">选择日期：</div>
                    <input type="text" id="start_loan_time" readonly="readonly" class="filtrate_input filtrate_date archiving_time customer_select_input" value="" placeholder="请选择日期" name="start_loan_time"/> <span style="color:#D8D8D8">—</span>
                    <input type="text" id="end_loan_time" readonly="readonly" class="filtrate_input filtrate_date archiving_time customer_select_input"  value="" placeholder="请选择日期"  name="end_loan_time"/>
                </div>
                <div class="filtrate_item select_item inline_block">
                    <div class="conditions_name inline_block" style="vertical-align: top;margin-left:18px;">打印状态：</div>
                    <select name=""  class="filtrate_select" id="merchantsState">
                        <option value="">全部</option>
                    </select>
                </div>
                <div class="filtrate_item date_conditions inline_block">
                    <div class="conditions_name inline_block">金额：</div>
                    <input type="text" class="filtrate_date archiving_time" value="" placeholder="请输入" /> —
                    <input type="text" class="filtrate_date archiving_time"  value="" placeholder="请输入" />
                    <div class="conditions_name inline_block">元</div>
                </div>
                <div class="filtrate_item select_item inline_block">
                    <div class="conditions_name inline_block" style="vertical-align: top;">付方账户：</div>
                    <select name=""  class="filtrate_select" id="merchantsTags" style="width:280px;">
                        <option value="">全部</option>
                    </select>
                </div>
                <div class="filtrate_item select_item inline_block">
                    <div class="conditions_name inline_block" style="vertical-align: top;">业务类型：</div>
                    <select name=""  class="filtrate_select" id="cityeSelect">
                        <option value="">代发</option>
                    </select>
                </div>
                <div class="btn_box text_right">
                    <a href="javascript:;" class="btn bg_btn cancel">清空</a>
                    <a href="javascript:" class="btn orange_btn confirm">筛选</a>
                </div>
            </div>
            <div class="merchants_list records_list" id="records_list">
                <table class="normal recordsNormal">
                    <colgroup>
                        <col style="width: 10%;" />
                        <col style="width: 25%;" />
                        <col style="width: 12%;" />
                        <col style="width: 12%;" />
                        <col style="width: 15%;" />
                        <col style="width: 15%;" />
                        <col style="width: 10%;" />
                        <col style="width: 15%;" />
                    </colgroup>
                    <thead>
                    <tr>
                        <th>业务类型</th>
                        <th>付方账户</th>
                        <th>总金额 (元)</th>
                        <th>日期 (经办日)</th>
                        <th>用途</th>
                        <th>业务请求结果</th>
                        <th>打印状态</th>
                        <th>操作</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr data-id="1">
                        <td title="">代代发发</td>
                        <td>北京，11241335432546，人民币</td>
                        <td>3545566.55</td>
                        <td>2018.04.23</td>
                        <td title="">广东车款垫款</td>
                        <td title="">银行处理中</td>
                        <td>未打印</td>
                        <td>
                            <a href="javascript:;" class="trans_print_btn trans_detail" title="查询详情" target="_blank"></a>
                            <a href="javascript:;" class="trans_print_btn print_btn" title="打印" target="_blank"></a>
                        </td>
                    </tr>
                    <tr data-id="2">
                        <td title="">代代发发</td>
                        <td>北京，11241335432546，人民币</td>
                        <td>3545566.55</td>
                        <td>2018.04.23</td>
                        <td title="">广东车款垫款</td>
                        <td title="">银行处理中</td>
                        <td>未打印</td>
                        <td>
                            <a href="javascript:;" class="trans_print_btn trans_detail" title="查询详情"></a>
                            <a href="javascript:;" class="trans_print_btn print_btn" title="打印"></a>
                        </td>
                    </tr>
                    </tbody>
                </table>
            </div>


            <!--分页部分 End-->
        </div>
        <!---- Part of Main info End ---->
    </div>
    <!-------- Part of main End -------->
</div>

{{include ('./../inc/jsSources')}}
<script src="{{markUri}}/static/dialog/dialog-layer.js"></script>
<script src="{{markUri}}/static/jedate/jquery.jedate.min.js"></script>
<script src="{{markUri}}/static/js/merchants.js"></script>
<script>
    (function ($) {
        $(function() {
            initDateStartEnd('#start_loan_time','#end_loan_time');  //日期筛选
        });
    })(jQuery,undefined);
</script>


</body>
</html>